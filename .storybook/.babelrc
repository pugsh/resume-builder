{
    "plugins": [
        [
            "babel-plugin-emotion",
            {
                "sourceMap": true,
                "autoLabel": true
            }
        ],
        "@babel/plugin-transform-react-display-name",
        [
            "@babel/plugin-proposal-decorators",
            {
                "decoratorsBeforeExport": false
            }
        ],
        "@babel/plugin-proposal-class-properties"
    ],
    "compact": false,
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false,
                "useBuiltIns": "usage"
            }
        ],
        "@babel/react"
    ]
}